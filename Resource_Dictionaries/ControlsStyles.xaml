    <SolidColorBrush x:Key="TitleButton.Disabled.Border" Color="Transparent"/>
    <SolidColorBrush x:Key="TitleButton.Disabled.Foreground" Color="#FF838383"/>
    <Style x:Key="MaxMinButton" TargetType="{x:Type Button}">
